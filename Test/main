#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x266af90 .scope module, "main" "main" 2 1;
 .timescale 0 0;
v0x266b080_0 .var "clk", 0 0;
    .scope S_0x266af90;
T_0 ;
    %vpi_call 2 5 "$dumpfile", "main.vcd";
    %vpi_call 2 6 "$dumpvars", 1'sb0, S_0x266af90;
    %vpi_call 2 7 "$display", "Hello world!";
    %delay 60, 0;
    %vpi_call 2 8 "$finish";
    %end;
    .thread T_0;
    .scope S_0x266af90;
T_1 ;
    %set/v v0x266b080_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x266af90;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x266b080_0, 1;
    %inv 8, 1;
    %set/v v0x266b080_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "main.v";
